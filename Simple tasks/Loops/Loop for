// Find: (1+sin0,1)*(1+sin0,2)+...+(1+sin10)
import java.util.Scanner;

import static java.lang.Math.*;
public class Pr2for {
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in); 
      
       double P=1;
       double e;
       double n=sc.nextDouble();
       for(int i=1;i<=10;i++){
           e=1+sin(i/10);
           P=P*e;
       }
       System.out.println(P);
    }     }
